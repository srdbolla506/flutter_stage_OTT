    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to running iOS on simulator - experimental",
            "type": "cordova",
            "request": "attach",
            "platform": "ios",
            "target": "emulator",
            "port": 9220,
            "sourceMaps": true,
            "cwd": "${workspaceFolder}"
        },
    {
        "name": "Attach to running iOS on device - experimental",
        "type": "cordova",
        "request": "attach",
        "platform": "ios",
        "target": "device",
        "port": 9220,
        "sourceMaps": true,
        "cwd": "${workspaceFolder}"
    },
        {
            "name": "stage_ott",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "stage_ott (debug mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug"
        },
        {
            "name": "stage_ott (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}